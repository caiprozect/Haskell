    "--Exercise 4.2\n",
    "insertionSort :: Ord a => [a] -> [a]\n",
    "insertionSort = foldl insert [] \n",
    "    where \n",
    "        insert [] x = [x]\n",
    "        insert (y : ys) x\n",
    "            | x <= y = x : y : ys\n",
    "            | otherwise = y : insert ys x"
   ]
